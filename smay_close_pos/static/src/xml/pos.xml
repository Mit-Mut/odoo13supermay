    <t t-name="ConfirmationClosePopupWidget">
		<div class="modal-dialog" id="sunny-test">
			<div class="popup popup-textinput" style="height:300px !important; width: 30%">
                <p class="title">
					<t t-esc=" widget.options.title || 'Confirmación de cierre'"/>
				</p>
                <t t-if='widget.options.session'>
					<div class="body">
						<table width="100%">
							<tr>
								<th style="text-align:left">
									Sesión
								</th>
                                <th style="text-align:left">
									<t t-esc="widget.options.session['name']"/>
								</th>
							</tr>
                            <tr>
								<th style="text-align:left">
									Cajero
								</th>
                                <th style="text-align:left">
									<t t-esc="widget.options.session['user_id'][1]"/>
								</th>
							</tr>
                            <tr>
								<t t-if="widget.options.session['cash_register_total_entry_encoding'] &lt;= 0">
									<th style="text-align:left">
										Sobrante
									</th>
								</t>
                                <t t-if="widget.options.session['cash_register_total_entry_encoding'] > 0">
									<th style="text-align:left">
										Faltante
									</th>
								</t>
                                <th style="text-align:left">
									$ <t
                                        t-esc="Math.round(Math.abs(widget.options.session['cash_register_total_entry_encoding'])*100,2)/100"/>
								</th>
							</tr>
                            <tr>
								<th colspan="2" style="font-size:17px;color:#170f7b;padding:30px 5px;text-align:left">
									<!--Al confirmar el cierre ya no se podran realizar ventas ni retiros.-->
                                    Da clic en aceptar para terminar el proceso de cierre.
								</th>
							</tr>
						</table>
					</div>
				</t>
                <div class="footer">
					<div class=" button acceptConfirmationClose">
                        Aceptar
					</div>
                    <!--                    <div class=" button cancelConfirmationClose">
                        Cancelar
                    </div>-->
				</div>
			</div>
		</div>
	</t>
</templates>
