<odoo>
    <data>
        <record id="res_users_form_view" model="ir.ui.view">
            <field name="name">res.users.form.view</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <!--<xpath expr="//notebook" position='inside'>-->
                <xpath expr="//group[@name='pos']" position="inside">
                    <page string='Compras Super May'>
                        <group>
                            <field name="x_almacen_compras" options="{'no_create': True,'no_open':True}"/>
                            <field name="x_cuenta_analitica" options="{'no_open':True,'no_create':True}"/>
                            <field name="purchase_validator"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>

        <record id="view_company_form" model="ir.ui.view">
            <field name="name">res.company.form</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='website']" position="after">
                    <field name="x_notification_partner_ids" widget="many2many_tags"/>
                </xpath>
            </field>
        </record>






        <record model="ir.ui.view" id="purchase_order_form_inherited">
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase_stock.purchase_order_view_form_inherit"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='picking_type_id']" position="replace">

                    <field name="picking_type_id" readonly="False" domain="[('code','=','incoming')]"
                           options="{'no_create': True,'no_open':True}" groups="stock.group_stock_multi_locations"/>

                </xpath>

            </field>
        </record>

        <record model="ir.ui.view" id="purchase_order_form_inherited3">
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//header" position="inside">
                    <button name="print_labels" type="object" string="Etiquetas cambio precio"
                            attrs="{'invisible':[('state','not in',('purchase','done'))]}" class="oe_highlight"/>
                </xpath>
                <xpath expr="//field[@name='product_id']" position="replace">
                    <field name="product_id"
                           options="{'no_create': True,'no_open':True}"
                           attrs="{'readonly': [('state', 'in', ('purchase', 'to approve','done', 'cancel'))]}"
                           context="{'partner_id':parent.partner_id, 'quantity':product_qty,'uom':product_uom, 'company_id': parent.company_id}"
                           force_save="1"/>

                </xpath>
                <xpath expr="//field[@name='partner_ref']" position="after">

                    <field name="cantidad_lineas" readonly="True"/>
                    <field name="cantidad_productos" readonly="True"/>
                    <field name="analytic_account_select" options="{'no_create': True,'no_open':True}" groups="purchase.group_purchase_manager"/>


                </xpath>
            </field>

        </record>

        <record model="ir.ui.view" id="purchase_order_form_inherited14">
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='button_unlock']" position="replace">

                </xpath>
            </field>

        </record>




        <record model="ir.ui.view" id="purchase_order_form_inherited">
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase_stock.purchase_order_view_form_inherit"/>
            <field name="groups_id" eval="[(6, 0, [ref('purchase.group_purchase_user')])]"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='picking_type_id']" position="replace">
                    <field name="picking_type_id" readonly="True" domain="[('code','=','incoming')]"
                           options="{'no_create': True,'no_open':True}" groups="stock.group_stock_multi_locations"/>

                </xpath>
            </field>

        </record>

        <record model="ir.ui.view" id="purchase_order_form_inherited14">
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='button_unlock']" position="replace">

                </xpath>
            </field>

        </record>

        <record model="ir.ui.view" id="purchase_order_form_inherited2">
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase_stock.purchase_order_view_form_inherit"/>
            <field name="groups_id" eval="[(6, 0, [ref('purchase.group_purchase_manager')])]"/>
            <field name="arch" type="xml">

                <!--<xpath expr="//field[@name='partner_ref']" position="after">
                    <field name="analytic_account_select" options="{'no_create': True,'no_open':True}"/>

                </xpath>-->

                <xpath expr="//field[@name='picking_type_id']" position="replace">
                    <field name="picking_type_id" readonly="False" domain="[('code','=','incoming')]"
                           options="{'no_create': True,'no_open':True}" groups="stock.group_stock_multi_locations"/>

                </xpath>
            </field>

        </record>



        <record model="ir.ui.view" id="product_template_form_view_inherited">
            <field name="name">product.template.utility</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='list_price']" position="after">
                    <field name="x_utility_percent"/>
                </xpath>
            </field>

        </record>
    </data>
</odoo>
