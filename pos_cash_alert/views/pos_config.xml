<odoo>
    <data>
        <record id="retirements_view_pos_config_form" model="ir.ui.view">
            <field name="name">view.pos.config.form_retirements</field>
            <field name="model">pos.config</field>
            <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet" position="inside">
                    <group col="2">
                        <field name='cash_withdraw' attrs="{'invisible': [('cash_control', '=', False)]}"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
