                                    </tree>
                                </field>
                            </group>

                            <group class="oe_subtotal_footer oe_right">
                                <button name="update_totals_managers" string=" Actualiza Totales"
                                        type="object" class="oe_link oe_left" groups="point_of_sale.group_pos_user"/>
                                <br/>
                                <field name="deposit_total"
                                       widget="monetary" string="Total Depositado"
                                       digits="(14,2)" readonly="1"/>
                                <br/>
                                <br/>
                                <field name="total_conciliado_banco"
                                       widget="monetary" string="Total Verificado en Banco"
                                       digits="(14,2)" readonly="1" groups="point_of_sale.group_pos_user"/>
                            </group>

                        </page>

                        <page string="RESUMEN">

                            <group>
                                <field name="validate_deposits_uid" readonly="1" string="Validados por:"
                                       options="{'no_open':True}"
                                       attrs="{'invisible':[('state','in',('borrador','nuevo','retiros_validados'))]}"/>
                                <field name="validate_deposits_date" readonly="1" string="Fecha de validación:"
                                       options="{'no_open':True}"
                                       attrs="{'invisible':[('state','in',('borrador','nuevo','retiros_validados'))]}"/>
                            </group>
                            <group class="oe_right">
                                <button name="update_totals_managers" string="Actualiza Totales"
                                        type="object" class="oe_link oe_left" groups="point_of_sale.group_pos_user"/>
                                <br/>
                                <field name="total_venta_bruta" widget="monetary" string="Venta Bruta"
                                       digits="(14,2)" readonly="1"/>
                                <field name="total_devoluciones" widget="monetary" string="Devoluciones"
                                       digits="(14,2)" readonly="1"/>

                                <field name="total_venta_neta" widget="monetary" string="Venta Neta"
                                       digits="(14,2)" readonly="1"/>


                                <br/>
                                <br/>

                                <field name="total_venta_neta" widget="monetary" string="Venta Neta"
                                       digits="(14,2)" readonly="1"/>
                                <field name="total_conciliado_card_payments" widget="monetary" string="Tarjetas ( - )"
                                       digits="(14,2)" readonly="1" groups="point_of_sale.group_pos_user"/>
                                <field name="total_gastos" widget="monetary" string="Gastos ( - )"
                                       digits="(14,2)" readonly="1" groups="point_of_sale.group_pos_user"/>
                                <field name="total_prestamos" widget="monetary" string="Prestamos ( - )"
                                       digits="(14,2)" readonly="1" groups="point_of_sale.group_pos_user"/>
                                <field name="total_faltantes_cajeros" widget="monetary" string="Faltantes ( - )"
                                       digits="(14,2)" readonly="1" groups="point_of_sale.group_pos_user"/>
                                <field name="total_sobrantes_cajeros" widget="monetary" string="Sobrantes ( + )"
                                       digits="(14,2)" readonly="1" groups="point_of_sale.group_pos_user"/>

                                <br/>
                                <br/>
                                <field name="gran_total" widget="monetary" string="Efectivo a Depositar ( = )"
                                       digits="(14,2)" readonly="1" groups="point_of_sale.group_pos_user"/>


                                <field name="deposit_total" widget="monetary" string="Enviado por conciliador $"
                                       digits="(14,2)" readonly="1" groups="point_of_sale.group_pos_user"/>

                                <field name="total_conciliado_banco" widget="monetary" string="Verificado en Banco $"
                                       digits="(14,2)" readonly="1" groups="point_of_sale.group_pos_user"/>

                                <br/>
                                <br/>


                                <field name="total_diferencia_banco" widget="monetary" string="DIFERENCIA BANCOS"
                                       digits="(14,2)" readonly="1" groups="point_of_sale.group_pos_user"/>


                            </group>


                        </page>
                    </notebook>
                </sheet>

            </xpath>
        </field>
    </record>

